					"};",
					"",
					"",
					"pm.sendRequest(getTokenRequest, (err, response) => {",
					"  const jsonResponse = response.json();",
					"  const newAccessToken = jsonResponse.accessToken;",
					"  pm.variables.set('access_token', newAccessToken);",
					"});"
				]
