{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server",
    "start-dev": "Set DOTENV_CONFIG_PATH=.env.development&node -r dotenv/config ./bin/www",
    "start-prod": "Set DOTENV_CONFIG_PATH=.env&node -r dotenv/config ./bin/www",
    "start": "Set DOTENV_CONFIG_PATH=.env&node -r dotenv/config ./bin/www",
    "test": "nyc mocha ./app_api/test --exit",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "bootstrap": "^5.1.0",
    "busboy": "^0.3.1",
    "connect-busboy": "0.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "gravatar": "^1.8.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "redis": "^3.1.2",
    "redis-rejson": "^1.0.0",
    "request": "^2.88.2",
    "stylus": "0.54.5",
    "tinymce": "^5.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "max-len": [
        2,
        120,
        4,
        {
          "ignoreUrls": true
        }
      ],
      "no-console": 0
    }
  },
  "eslintIgnore": [
    "node_modules",
    "graphQL",
    "routes"
  ]
}
